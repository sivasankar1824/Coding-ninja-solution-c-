 PRACTICE PROBLEMS
 1,Find area of rectangle.
 
 #include <iostream>
using namespace std;

int main() {
    int l,b;
    scanf("%d %d",&l,&b);
    printf("%d",l*b);

    return 0;
}
2,Print Name and age

#include <iostream>
using namespace std;

int main() {
    string c;
    int n;
    cin>>c;
    cin>>n;
    cout<<"The name of the person is "<< c<<" and the age is "<<n<<".";

    return 0;
}
3,Swap Two Numbers

#include <bits/stdc++.h> 
#include <utility>

pair < int, int > swap(pair < int, int > swapValues) {
    // Write your code here.
     int k = swapValues.first;
    swapValues.first = swapValues.second;
    swapValues.second = k;
    return swapValues;
}
4,Calculate Simple Interest

#include <bits/stdc++.h> 
#include <iostream>
using namespace std;

int main() {
    //Write your code here
    int p,t;
    float i;
    cin>>p;
    cin>>i;
    cin>>t;
    cout<<floor((p*i*t)/100);

    return 0;
}

